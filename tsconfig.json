{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "baseUrl": "src",
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./node_modules/@types", "./src/@types/express"],
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules/*", "dist/", "prisma/"]
}
